<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="2.0"
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
    <converter>
        <converter-id>datePeriod</converter-id>
        <converter-class>jm.com.dpbennett.entity.DatePeriodConverter</converter-class>
    </converter>
    <converter>
        <converter-id>departmentConverter</converter-id>
        <converter-class>jm.com.dpbennett.entity.DepartmentConverter</converter-class>
    </converter>
    <converter>
        <converter-id>businessOfficeConverter</converter-id>
        <converter-class>jm.com.dpbennett.entity.BusinessOfficeConverter</converter-class>
    </converter>
    <converter>
        <converter-id>employeeConverter</converter-id>
        <converter-class>jm.com.dpbennett.entity.EmployeeConverter</converter-class>
    </converter>
    <converter>
        <converter-id>contactConverter</converter-id>
        <converter-class>jm.com.dpbennett.entity.ContactConverter</converter-class>
    </converter>
    <converter>
        <converter-id>clientConverter</converter-id>
        <converter-class>jm.com.dpbennett.entity.ClientConverter</converter-class>
    </converter>
    <navigation-rule>
        <from-view-id>/compliance_main.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>market_survey</from-outcome>
            <to-view-id>/ProductSurvey.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
</faces-config>
