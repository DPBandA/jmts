	<queryString>
		<![CDATA[SELECT
     job.`ID` AS job_ID,
     job.`DEPARTMENT_ID` AS job_DEPARTMENT_ID,
     job.`JOBSTATUSANDTRACKING_ID` AS job_JOBSTATUSANDTRACKING_ID,
     job.`CLIENT_ID` AS job_CLIENT_ID,
     jobstatusandtracking.`ID` AS jobstatusandtracking_ID,
     jobstatusandtracking.`DATESUBMITTED` AS jobstatusandtracking_DATESUBMITTED,
     client.`ID` AS client_ID,
     client.`NAME` AS client_NAME,
     department.`ID` AS department_ID,
     department.`NAME` AS department_NAME,
     department_A.`ID` AS department_A_ID,
     department_A.`NAME` AS department_A_NAME,
     jobstatusandtracking.`WORKPROGRESS` AS jobstatusandtracking_WORKPROGRESS,
     job.`JOBNUMBER` AS job_JOBNUMBER,
     job.`INSTRUCTIONS` AS job_INSTRUCTIONS
FROM
     `jobstatusandtracking` jobstatusandtracking INNER JOIN `job` job ON jobstatusandtracking.`ID` = job.`JOBSTATUSANDTRACKING_ID`
     INNER JOIN `client` client ON job.`CLIENT_ID` = client.`ID`
     INNER JOIN `department` department ON job.`DEPARTMENT_ID` = department.`ID`
     INNER JOIN `department` department_A ON job.`SUBCONTRACTEDDEPARTMENT_ID` = department_A.`ID`
WHERE
     ((jobstatusandtracking.`DATESUBMITTED` >= $P{startOFPeriod}
 AND jobstatusandtracking.`DATESUBMITTED` <= $P{endOFPeriod})
 AND (department.`ID` = 25
  OR department_A.`ID` = 24
  OR department.`ID` = 24
  OR department_A.`ID` = 25)
 AND (client.`NAME` LIKE 'Salada%'
  OR client.`NAME` LIKE 'Garth Dawkins%'
  OR client.`NAME` LIKE 'Scientific Research%'))
ORDER BY
     job.`ID` DESC]]>
	</queryString>
