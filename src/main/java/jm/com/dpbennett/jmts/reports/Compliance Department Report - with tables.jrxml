	<subDataset name="Import Monitoring - One Stop Shop" uuid="b8eca096-8286-47a5-89db-e37ae16138e5">
		<parameter name="startDate" class="java.util.Date" isForPrompting="false"/>
		<parameter name="endDate" class="java.util.Date" isForPrompting="false"/>
		<queryString>
			<![CDATA[SELECT
     GROUP_CONCAT(marketproduct.`NAME` SEPARATOR ', ') AS products,
     GROUP_CONCAT(marketproductsurvey.`PORTOFENTRY` SEPARATOR ', ') AS location,
     GROUP_CONCAT(marketproductsurvey.`CONTAINER` SEPARATOR ', ') AS containerNums,
     client.`ID` AS client_ID,
     client.`NAME` AS client_NAME,
     marketproductsurvey.`ID` AS marketproductsurvey_ID,
     marketproductsurvey.`COMMENTS` AS marketproductsurvey_COMMENTS,
     marketproductsurvey.`CONTAINER` AS marketproductsurvey_CONTAINER,
     marketproductsurvey.`DATEOFSURVEY` AS marketproductsurvey_DATEOFSURVEY,
     marketproductsurvey.`DATESIGNED` AS marketproductsurvey_DATESIGNED,
     marketproductsurvey.`NAME` AS marketproductsurvey_NAME,
     marketproductsurvey.`PORTOFENTRY` AS marketproductsurvey_PORTOFENTRY,
     marketproductsurvey.`SURVEYENDTIME` AS marketproductsurvey_SURVEYENDTIME,
     marketproductsurvey.`SURVEYSTARTTIME` AS marketproductsurvey_SURVEYSTARTTIME,
     marketproductsurvey.`SURVEYTYPE` AS marketproductsurvey_SURVEYTYPE,
     marketproductsurvey.`INSPECTOR_ID` AS marketproductsurvey_INSPECTOR_ID,
     marketproductsurvey.`RETAILOUTLET_ID` AS marketproductsurvey_RETAILOUTLET_ID,
     marketproductsurvey.`RETAILREPRESENTATIVE_ID` AS marketproductsurvey_RETAILREPRESENTATIVE_ID,
     employee.`ID` AS employee_ID,
     employee.`FIRSTNAME` AS employee_FIRSTNAME,
     employee.`LASTNAME` AS employee_LASTNAME,
     employee.`NAME` AS employee_NAME,
     marketproductsurvey_marketproduct.`MarketProductSurvey_ID` AS marketproductsurvey_marketproduct_MarketProductSurvey_ID,
     marketproductsurvey_marketproduct.`products_ID` AS marketproductsurvey_marketproduct_products_ID,
     marketproduct.`ID` AS marketproduct_ID,
     marketproduct.`BRAND` AS marketproduct_BRAND,
     marketproduct.`COMMENTS` AS marketproduct_COMMENTS,
     marketproduct.`COUNTRYOFORIGIN` AS marketproduct_COUNTRYOFORIGIN,
     marketproduct.`TYPE` AS marketproduct_TYPE,
     marketproduct.`NAME` AS marketproduct_NAME
FROM
     `client` client INNER JOIN `marketproductsurvey` marketproductsurvey ON client.`ID` = marketproductsurvey.`RETAILOUTLET_ID`
     INNER JOIN `employee` employee ON marketproductsurvey.`INSPECTOR_ID` = employee.`ID`
     INNER JOIN `marketproductsurvey_marketproduct` marketproductsurvey_marketproduct ON marketproductsurvey.`ID` = marketproductsurvey_marketproduct.`MarketProductSurvey_ID`
     INNER JOIN `marketproduct` marketproduct ON marketproductsurvey_marketproduct.`products_ID` = marketproduct.`ID`
WHERE
     marketproductsurvey.`DATEOFSURVEY` >= $P{startDate}
 AND marketproductsurvey.`DATEOFSURVEY` <= $P{endDate}
GROUP BY
     client.`NAME`
ORDER BY
     client.`NAME` ASC]]>
		</queryString>
