	<queryString>
		<![CDATA[SELECT jmts.purchaserequisition.`ID`,
	jmts.purchaserequisition.`DESCRIPTION`,
	jmts.purchaserequisition.`DATEREQUIRED`,
	jmts.purchaserequisition.`NUMBER`,
	jmts.purchaserequisition.`PRIORITYCODE`,
	jmts.purchaserequisition.`PURCHASEORDERDATE`,
	jmts.purchaserequisition.`PURCHASEORDERNUMBER`,
	jmts.purchaserequisition.`REQUISITIONDATE`,
	jmts.purchaserequisition.`ORIGINATINGDEPARTMENT_ID`,
	jmts.purchaserequisition.`ORIGINATOR_ID`,
	jmts.supplier.`NAME` AS supplierName
FROM jmts.purchaserequisition
	INNER JOIN jmts.supplier ON 
	 jmts.purchaserequisition.`SUPPLIER_ID` = jmts.supplier.`ID` 
WHERE 
	 jmts.purchaserequisition.`ID` = $P{prId}]]>
	</queryString>
