	<subDataset name="Document Inspections" uuid="6a4f1065-60c7-455d-b1a0-6537d77b24ad">
		<parameter name="inspectionPoint" class="java.lang.String" isForPrompting="false">
			<defaultValueExpression><![CDATA["One Stop Shop"]]></defaultValueExpression>
		</parameter>
		<parameter name="startOfPeriod" class="java.util.Date" isForPrompting="false">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="endOfPeriod" class="java.util.Date" isForPrompting="false">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="location" class="java.lang.String" isForPrompting="false"/>
		<queryString>
			<![CDATA[SELECT
     compliancesurvey.`ID` AS compliancesurvey_ID,
     compliancesurvey.`CONSIGNEE_ID` AS compliancesurvey_CONSIGNEE_ID,
     client.`ID` AS client_ID,
     client.`NAME` AS client_NAME,
     entrydocumentinspection.`ID` AS entrydocumentinspection_ID,
     entrydocumentinspection.`DOCUMENTSTAMPED` AS entrydocumentinspection_DOCUMENTSTAMPED,
     entrydocumentinspection.`ENTRYDOCUMENTNUMBER` AS entrydocumentinspection_ENTRYDOCUMENTNUMBER,
     entrydocumentinspection.`COUNTRYOFCONSIGNMENT` AS entrydocumentinspection_COUNTRYOFCONSIGNMENT,
     SUM(IF(entrydocumentinspection.`DOCUMENTSTAMPED`="To be Inspected",1,0)) AS stampedtobeinspected,
     SUM(IF(entrydocumentinspection.`DOCUMENTSTAMPED`="Seen",1,0)) AS stampedseen
FROM
     `compliancesurvey` compliancesurvey INNER JOIN `client` client ON compliancesurvey.`CONSIGNEE_ID` = client.`ID`
     INNER JOIN `entrydocumentinspection` entrydocumentinspection ON compliancesurvey.`ENTRYDOCUMENTINSPECTION_ID` = entrydocumentinspection.`ID`
WHERE
     compliancesurvey.`DATEOFSURVEY` >= $P{startOfPeriod}
 AND compliancesurvey.`DATEOFSURVEY` <= $P{endOfPeriod}
GROUP BY
     client.`ID`]]>
		</queryString>
		<field name="compliancesurvey_ID" class="java.lang.Long"/>
		<field name="compliancesurvey_CONSIGNEE_ID" class="java.lang.Long"/>
		<field name="client_ID" class="java.lang.Long"/>
		<field name="client_NAME" class="java.lang.String"/>
		<field name="entrydocumentinspection_ID" class="java.lang.Long"/>
		<field name="entrydocumentinspection_DOCUMENTSTAMPED" class="java.lang.String"/>
		<field name="entrydocumentinspection_ENTRYDOCUMENTNUMBER" class="java.lang.String"/>
		<field name="entrydocumentinspection_COUNTRYOFCONSIGNMENT" class="java.lang.String"/>
		<field name="stampedtobeinspected" class="java.lang.Long"/>
		<field name="stampedseen" class="java.lang.Long"/>
		<variable name="totalStampedToBeInspected" class="java.lang.Integer" calculation="Sum">
			<variableExpression><![CDATA[$F{stampedtobeinspected}]]></variableExpression>
			<initialValueExpression><![CDATA[0]]></initialValueExpression>
		</variable>
		<variable name="totalStampedDocumentSeen" class="java.lang.Integer" calculation="Sum">
			<variableExpression><![CDATA[$F{stampedseen}]]></variableExpression>
			<initialValueExpression><![CDATA[0]]></initialValueExpression>
		</variable>
	</subDataset>
	<subDataset name="Location Visits" uuid="bc60b3b9-0ea2-4058-bd06-4b401e1c9459">
		<parameter name="startOfPeriod" class="java.util.Date" isForPrompting="false"/>
		<parameter name="endOfPeriod" class="java.util.Date" isForPrompting="false"/>
		<queryString>
			<![CDATA[SELECT
     compliancesurvey.`ID` AS compliancesurvey_ID,
     compliancesurvey.`COMMENTS` AS compliancesurvey_COMMENTS,
     compliancesurvey.`DATEOFSURVEY` AS compliancesurvey_DATEOFSURVEY,
     compliancesurvey.`INSPECTIONPOINT` AS compliancesurvey_INSPECTIONPOINT,
     compliancesurvey.`PORTOFENTRY` AS compliancesurvey_PORTOFENTRY,
     compliancesurvey.`CONSIGNEE_ID` AS compliancesurvey_CONSIGNEE_ID,
     client.`ID` AS client_ID,
     client.`NAME` AS client_NAME,
     COUNT(compliancesurvey.`INSPECTIONPOINT`) AS visits
FROM
     `client` client INNER JOIN `compliancesurvey` compliancesurvey ON client.`ID` = compliancesurvey.`CONSIGNEE_ID`
WHERE
     compliancesurvey.`DATEOFSURVEY` >= $P{startOfPeriod}
 AND compliancesurvey.`DATEOFSURVEY` <= $P{endOfPeriod}
GROUP BY
     compliancesurvey.`INSPECTIONPOINT`]]>
		</queryString>
		<field name="compliancesurvey_ID" class="java.lang.Long"/>
		<field name="compliancesurvey_COMMENTS" class="java.lang.String"/>
		<field name="compliancesurvey_DATEOFSURVEY" class="java.sql.Date"/>
		<field name="compliancesurvey_INSPECTIONPOINT" class="java.lang.String"/>
		<field name="compliancesurvey_PORTOFENTRY" class="java.lang.String"/>
		<field name="compliancesurvey_CONSIGNEE_ID" class="java.lang.Long"/>
		<field name="client_ID" class="java.lang.Long"/>
		<field name="client_NAME" class="java.lang.String"/>
		<field name="visits" class="java.lang.Long"/>
		<variable name="totalNumOfVisits" class="java.lang.Integer" calculation="Sum">
			<variableExpression><![CDATA[$F{visits}]]></variableExpression>
			<initialValueExpression><![CDATA[0]]></initialValueExpression>
		</variable>
		<group name="compliancesurvey_INSPECTIONPOINT">
			<groupExpression><![CDATA[$F{compliancesurvey_INSPECTIONPOINT}]]></groupExpression>
		</group>
	</subDataset>
	<subDataset name="Containers Inspected" uuid="88a0f67c-5674-49a7-bb22-6572740cc2ef">
		<parameter name="startOfPeriod" class="java.util.Date" isForPrompting="false"/>
		<parameter name="endOfPeriod" class="java.util.Date"/>
		<queryString>
			<![CDATA[SELECT
(LENGTH(entrydocumentinspection.`CONTAINERNUMBERS`)-LENGTH(REPLACE(entrydocumentinspection.`CONTAINERNUMBERS`,',',''))+1) AS numContainers,
     compliancesurvey.`ID` AS compliancesurvey_ID,
     compliancesurvey.`DATEOFSURVEY` AS compliancesurvey_DATEOFSURVEY,
     entrydocumentinspection.`CONTAINERNUMBERS` AS entrydocumentinspection_CONTAINERNUMBERS
FROM
     `compliancesurvey` compliancesurvey INNER JOIN `compliancesurvey_productinspection` compliancesurvey_productinspection ON compliancesurvey.`ID` = compliancesurvey_productinspection.`ComplianceSurvey_ID`
     INNER JOIN `productinspection` productinspection ON compliancesurvey_productinspection.`productInspections_ID` = productinspection.`ID`
     INNER JOIN `entrydocumentinspection` entrydocumentinspection ON compliancesurvey.`ENTRYDOCUMENTINSPECTION_ID` = entrydocumentinspection.`ID`
WHERE
          compliancesurvey.`DATEOFSURVEY` >= $P{startOfPeriod}
 AND compliancesurvey.`DATEOFSURVEY` <= $P{endOfPeriod}
GROUP BY compliancesurvey.`ID`]]>
		</queryString>
		<field name="numContainers" class="java.lang.Long"/>
		<field name="compliancesurvey_ID" class="java.lang.Long"/>
		<field name="compliancesurvey_DATEOFSURVEY" class="java.sql.Date"/>
		<field name="entrydocumentinspection_CONTAINERNUMBERS" class="java.lang.String"/>
		<variable name="totalContainers" class="java.lang.Integer" calculation="Sum">
			<variableExpression><![CDATA[$F{numContainers}]]></variableExpression>
			<initialValueExpression><![CDATA[0]]></initialValueExpression>
		</variable>
	</subDataset>
	<subDataset name="Detention Requests" uuid="453c35b8-d30b-4dc9-beb2-066a4115438e">
		<parameter name="startOfPeriod" class="java.util.Date" isForPrompting="false"/>
		<parameter name="endOfPeriod" class="java.util.Date" isForPrompting="false"/>
		<queryString>
			<![CDATA[SELECT
     compliancesurvey.`DATEOFSURVEY` AS compliancesurvey_DATEOFSURVEY,
     compliancesurvey.`ID` AS compliancesurvey_ID,
     compliancesurvey.`REQUESTFORDETENTIONISSUEDFORPORTOFENTRY` AS compliancesurvey_REQUESTFORDETENTIONISSUEDFORPORTOFENTRY,
     client.`NAME` AS client_NAME,
     compliancesurvey.`DATEOFDETENTION` AS compliancesurvey_DATEOFDETENTION,
     COUNT(*) AS detentions
FROM
     `client` client INNER JOIN `compliancesurvey` compliancesurvey ON client.`ID` = compliancesurvey.`CONSIGNEE_ID`
WHERE
     compliancesurvey.`DATEOFDETENTION` >= $P{startOfPeriod}
 AND compliancesurvey.`DATEOFDETENTION` <= $P{endOfPeriod}
AND compliancesurvey.`REQUESTFORDETENTIONISSUEDFORPORTOFENTRY`]]>
		</queryString>
		<field name="compliancesurvey_DATEOFSURVEY" class="java.sql.Date"/>
		<field name="compliancesurvey_ID" class="java.lang.Long"/>
		<field name="compliancesurvey_REQUESTFORDETENTIONISSUEDFORPORTOFENTRY" class="java.lang.Boolean"/>
		<field name="client_NAME" class="java.lang.String"/>
		<field name="compliancesurvey_DATEOFDETENTION" class="java.sql.Date"/>
		<field name="detentions" class="java.lang.Long"/>
		<variable name="totalDetentions" class="java.lang.Integer" calculation="Sum">
			<variableExpression><![CDATA[$F{detentions}]]></variableExpression>
			<initialValueExpression><![CDATA[0]]></initialValueExpression>
		</variable>
	</subDataset>
	<subDataset name="Samples and Units Collected" uuid="11d27ce0-e5a3-475d-ab98-dacb15041a06">
		<parameter name="startOfPeriod" class="java.util.Date" isForPrompting="false"/>
		<parameter name="endOfPeriod" class="java.util.Date" isForPrompting="false"/>
		<queryString>
			<![CDATA[SELECT
     SUM(productinspection.`NUMPRODUCTSSAMPLED`) AS productinspection_NUMPRODUCTSSAMPLED,
     COUNT(productinspection.`NAME`) as samples,
     compliancesurvey.`DATEOFSURVEY` AS compliancesurvey_DATEOFSURVEY,
     compliancesurvey_productinspection.`ComplianceSurvey_ID` AS compliancesurvey_productinspection_ComplianceSurvey_ID,
     compliancesurvey_productinspection.`productInspections_ID` AS compliancesurvey_productinspection_productInspections_ID,
     productinspection.`NAME` AS productinspection_NAME,
     client.`NAME` AS client_NAME,
     client.`ID` AS client_ID
FROM
     `compliancesurvey` compliancesurvey INNER JOIN `compliancesurvey_productinspection` compliancesurvey_productinspection ON compliancesurvey.`ID` = compliancesurvey_productinspection.`ComplianceSurvey_ID`
     INNER JOIN `productinspection` productinspection ON compliancesurvey_productinspection.`productInspections_ID` = productinspection.`ID`
     INNER JOIN `client` client ON compliancesurvey.`CONSIGNEE_ID` = client.`ID`
WHERE
     compliancesurvey.`DATEOFSURVEY` >= $P{startOfPeriod}
 AND compliancesurvey.`DATEOFSURVEY` <= $P{endOfPeriod}
 AND productinspection.`NUMPRODUCTSSAMPLED` > 0
GROUP BY
     client.`ID`]]>
		</queryString>
		<field name="productinspection_NUMPRODUCTSSAMPLED" class="java.math.BigDecimal"/>
		<field name="samples" class="java.lang.Long"/>
		<field name="compliancesurvey_DATEOFSURVEY" class="java.sql.Date"/>
		<field name="compliancesurvey_productinspection_ComplianceSurvey_ID" class="java.lang.Long"/>
		<field name="compliancesurvey_productinspection_productInspections_ID" class="java.lang.Long"/>
		<field name="productinspection_NAME" class="java.lang.String"/>
		<field name="client_NAME" class="java.lang.String"/>
		<field name="client_ID" class="java.lang.Long"/>
		<variable name="totalSamplesCollected" class="java.lang.Integer" calculation="Sum">
			<variableExpression><![CDATA[$F{samples}]]></variableExpression>
			<initialValueExpression><![CDATA[0]]></initialValueExpression>
		</variable>
		<variable name="totalProductUnitsCollected" class="java.lang.Integer" calculation="Sum">
			<variableExpression><![CDATA[$F{productinspection_NUMPRODUCTSSAMPLED}]]></variableExpression>
			<initialValueExpression><![CDATA[0]]></initialValueExpression>
		</variable>
	</subDataset>
