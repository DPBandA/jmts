	<queryString>
		<![CDATA[SELECT
     client.`ID` AS client_ID,
     client.`TAG` AS client_TAG,
     client.`TAXREGISTRATIONNUMBER` AS client_TAXREGISTRATIONNUMBER,
     client.`NUMBER` AS client_NUMBER,
     client.`TYPE` AS client_TYPE,
